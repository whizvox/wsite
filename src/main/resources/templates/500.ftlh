<#include "macros.ftlh">
<@htmlBase "500" siteName>
  <section id="500">
    <h1>500 - Internal Server Error</h1>
    <p>The server could not complete the request as an error has occurred. Please report this to the webmaster.</p>
    <section class="stacktrace">
      ${stacktrace}
    </section>
  </section>
</@htmlBase>